.grid-33
  .app--section
    .app--user--list
      each friend in friends
        .circle--fluid
          .circle--fluid--cell.circle-fluid--primary
            a.app--tweet--author
              div.app--avatar
                img(src='#{friend.photo}')
              h4= friend.name
              p= friend.atURL
          .circle--fluid--cell
            a.button.button-text Unfollow TEXT ONLY

.grid-33
  .app--section
    ul.app--user--list
      each friend in friends
        li
          .circle--fluid
            .circle--fluid--cell.circle-fluid--primary
              a.app--tweet--author
                div.app--avatar(style= 'background-image: url('+ friend.photo + ')')
                  img(src= friend.photo)
                h4= friend.name
                p= friend.url
            .circle--fluid--cell
              a.button.button-text Unfollow
